{
  "manifest_version": 2,

  "name": "Americas Test Kitchen Search",
  "description": "This extension goes to search at americastestkitchen",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
